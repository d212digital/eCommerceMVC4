@if (AppDataHelper.IsRTL)
{
    <link href="https://fonts.googleapis.com/css2?family=Tajawal:wght@400;700&display=swap" rel="stylesheet">
   
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js"></script>


    @Scripts.Render("~/bundles/content/rtl/hscripts")
}
else
{
    <link href="https://fonts.googleapis.com/css2?family=Karla:wght@400;700;800&display=swap" rel="stylesheet">
  

    @Styles.Render("~/bundles/content/webfrontend/css")
    @Scripts.Render("~/bundles/content/web/headerscripts")
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js"></script>

}